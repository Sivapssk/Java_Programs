class d{
    public static void main(String args[]){
        int r=7;
        int c=5;
        for(int i=0;i<r;i++){
            for(int j=0;j<c;j++){
                System.out.print("*");
                if(j!=4)
                System.out.print("-");
            }
            System.out.println("");
        }
    }
}
