class d{
    public static void main(String[] args){
        int ar[]={5,9,1,8,7};
        
        int n=ar.length;
        for(int i=0;i<n;i++){
            for(int j=i;j<n;j++){
                for(int k=i;k<=j;k++){
                    System.out.print(ar[k]+" ");
                }
                System.out.println();
            }
            
        }
    }
}
