class d{
    public static void main(String args[]){
       String je="aA";
       String s="aAAbbb";
       int count=0;
       for(int i=0;i<je.length();i++){
        for(int j=0;j<s.length();j++){
            if(je.charAt(i)==s.charAt(j)){
                count++;
                
            }
        }
       }
       System.out.println(count);
    }
}
