import java.util.*;
class d{
    public static void main(String args[]){
        int ar[]={10,2,1,7,0,9};
        int n=ar.length;
        for(int i=0;i<n;i++){
            for(int j=0;j<n-i-1;j++){
                if(ar[j]>ar[j+1]){
                    int temp=ar[j];
                    ar[j]=ar[j+1];
                    ar[j+1]=temp;
                }
            }
        }
        System.out.print(Arrays.toString(ar));
    
    }
}
