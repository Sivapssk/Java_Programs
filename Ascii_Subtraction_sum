class d{
    public static void main(String args[]){
        String s="hello";
        int temp;
        int sum=0;
        for(int i=0;i<s.length()-1;i++){
            temp=Math.abs(s.charAt(i)- s.charAt(i+1));
            sum=sum+temp;
            
        }
        System.out.println(sum);
    }
}
