class d{
    public static void main(String args[]){
        int ar[]={2,3,4,5,6};
        int n=ar.length;
        int sa=3;
        int res=0;
        int l=0;
        int temp=0;
        for(int r=0;r<n;r++){
            
            temp=temp+ar[r];
            if(r-l==sa){
                temp=temp-ar[l];
                l++;
            }
            if(r-l+1==sa){
                res=Math.max(res,temp);
            }
        }
        System.out.println(res);
    }
}
