class d{
    public static void main(String args[]){
       int array[]={4,46,1,50,27,83};
       int max=array[0];//can also take 0
       int min=array[0];
       for(int i=0;i<array.length;i++){
        if(array[i]>max){
            max=array[i];
        }
        if(array[i]<min)
        min=array[i];
        
       }
       System.out.println(max);
       System.out.println(min);
    }
}
