class d{
    public static void main(String args[]){
        String s="ada";
        String ans="";
        //string reverse
        for(int i=s.length()-1;i>=0;i--){
            ans=ans+s.charAt(i);
            
        }
        //palandrome or not
        if(ans.equals(s)){
            System.out.print("pala");
        }
        else{
            System.out.print("not pal");
        }
        }
}
