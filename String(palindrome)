class d{
//effective way of palindrome program
    public static void main(String args[]){
        String s="sivis";
        int l=s.length();
        boolean palindrome=true;
        for(int i=0;i<l/2;i++){
            if(s.charAt(i)!=s.charAt(l-i-1)){
                palindrome=false;
            }
        }
        if(palindrome){
            System.out.println("palindrome");
        }
        else{
            System.out.println("not palindrome");

        }
    }
}
