class d{
    public static void main(String args[]){
        String s="255.244..1.422";
        String ans="";
        for(int i=0;i<s.length();i++){
            if(s.charAt(i)=='.'){
                ans=ans+"[.]";
            }
            else{
                ans=ans+s.charAt(i);
            }
        }
        System.out.println(ans);
        
    }
}
//255[.]244[.][.]1[.]422
