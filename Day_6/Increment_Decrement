class d{
    public static void main(String args[]){
        String []s={"++X","--X","X++","++X"};
        int ans=0;
        for(int i=0;i<s.length;i++){
            String temp=s[i];
            if(temp.equals("++X")||temp.equals("X++")){
                ans++;
            }else{
                ans--;
            }
        }
        System.out.println(ans);
        
    }
}
